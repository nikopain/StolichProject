<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cl.usm.stolichproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:label="Menu Principal" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="Juego" android:name=".GameActivity" />
        <activity android:label="Opciones" android:name=".OptionsActivity" />
        <activity android:label="Instrucciones" android:name=".InstructionsActivity"></activity>
    </application>

</manifest>
